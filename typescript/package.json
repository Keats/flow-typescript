{
  "name": "react-example",
  "version": "0.0.0",
  "description": "A react example",
  "scripts": {
    "clean": "rm -rf dist && rm -rf build",
    "webpack:prod": "webpack -p --config webpack.production.config.js",
    "build": "npm run clean && npm run lint && npm run sass && webpack",
    "dist": "npm run clean && npm run lint && NODE_ENV=production npm run sass && npm run webpack:prod",
    "lint": "eslint src",
    "start": "./bin/dev",
    "sass": "gulp sass",
    "sass-watch": "gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Keats/react-example.git"
  },
  "author": "Vincent Prouillet",
  "bugs": {
    "url": "https://github.com/Keats/react-example/issues"
  },
  "homepage": "https://github.com/Keats/react-example#readme",
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.0.3",
    "babel-runtime": "^5.8.20",
    "chai": "^3.2.0",
    "eslint": "^1.0.0",
    "eslint-config-airbnb": "0.0.7",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.2.2",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^3.0.1",
    "gulp-sass": "^2.0.4",
    "html-webpack-plugin": "^1.6.1",
    "react-transform-webpack-hmr": "^0.1.4",
    "redux-devtools": "^2.1.0",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "history": "^1.9.1",
    "immutable": "^3.7.4",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "react-redux": "^2.1.1",
    "react-router": "1.0.0-rc1",
    "redux": "^3.0.0",
    "redux-router": "1.0.0-beta3",
    "redux-thunk": "^1.0.0"
  }
}
